{
  "tsserver.npm": "/Users/ussirin/.nvm/versions/node/v16.8.0/bin/npm",
  "tsserver.typingsCacheLocation": "~/.cache/coc-tsserver",
  "tsserver.log": "verbose",
  "tsserver.trace.server": "verbose",
  "tsserver.maxTsServerMemory": 12288,
  "tsserver.formatOnType": true,

  "typescript.format.enabled": true,
  "typescript.suggestionActions.enabled": true,
  "javascript.inlayHints.parameterTypes.enabled": true,
  "typescript.inlayHints.variableTypes.enabled": true,

  "tslint.autoFixOnSave": true,
  "tslint.suppressWhileTypeErrorsPresent": false,

  "codeLens.enable": false,
  "codeLens.separator": "‣",

  "list.indicator": "❯",

  "diagnostic.enable": true,
  "diagnostic.errorSign": "⨯",
  "diagnostic.checkCurrentLine": true,
  "diagnostic.virtualText": false,
  "diagnostic.enableSign": true,
  "diagnostic.enableHighlightLineNumber": true,

  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "go",
    "typescript",
    "typescriptreact",
    "javascript",
    "cs",
    "javascriptreact",
    "lua",
    "scss",
    "json"
  ],
  "signature.preferShownAbove": true,
  "suggest.maxCompleteItemCount": 250,

  "javascript.validate.enable": true,

  "sumneko-lua.enableNvimLuaDev": true,

  "Lua.completion.enable": true,
  "Lua.diagnostics.globals": ["vim"],
  "Lua.workspace.library": [
    "/opt/homebrew/Cellar/neovim/HEAD-d7a7315/share/nvim/runtime",
    "/opt/homebrew/Cellar/neovim/HEAD-d7a7315/share/nvim/runtime/lua",
    "/opt/homebrew/Cellar/neovim/HEAD-d7a7315/share/nvim/runtime/lua/vim",
    "/opt/homebrew/Cellar/neovim/HEAD-d7a7315/share/nvim/runtime/lua/vim/lsp"
  ],
  "Lua.format.enable": true,
  "Lua.hint.enable": false,
  "Lua.completion.displayContext": 10,
  "Lua.telemetry.enable": false,

  "languageserver": {
    "golangci-lint-languageserver": {
      "command": "golangci-lint-langserver",
      "filetypes": ["go"],
      "initializationOptions": {
        "command": ["golangci-lint", "run", "--out-format", "json"]
      }
    }
  },
  "dialog.floatBorderHighlight": "CocFloatingBorder",
  "diagnostic.floatConfig": {
    "border": true,
    "rounded": true,
    "shadow": false
  },
  "suggest.pumFloatConfig": {
    "border": true,
    "rounded": true,
    "shadow": false
  },
  "signature.floatConfig": {
    "border": true,
    "rounded": true,
    "shadow": false
  },
  "hover.floatConfig": {
    "border": true,
    "rounded": true,
    "shadow": false
  }
}
